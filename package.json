{
  "name": "electron-react-router-boilerplate",
  "version": "0.7.0",
  "description": "Electron React React-Router boilerplate",
  "author": "tcerdaITBA",
  "homepage": "https://github.com/tcerdaITBA/electron-react-router-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/tcerdaITBA/electron-react-router-boilerplate.git"
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "icon": "./public/icons/mac/icon.icns",
      "category": "public.app-category.utilities"
    },
    "files": [
      "./build/**/*",
      "./dist/**/*",
      "./node_modules/**/*",
      "./public/**/*",
      "*.js"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "prod": "webpack --config webpack.build.config.js && electron --noDevServer .",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "build": "webpack --config webpack.build.config.js",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "electron-packager ./ --out=./builds --overwrite"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "classnames": "latest",
    "css-loader": "^1.0.0",
    "dotenv-webpack": "^1.5.7",
    "electron": "^7.2.4",
    "electron-packager": "^12.2.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-excel-renderer": "^1.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "ajv": "^6.5.4",
    "bootstrap": "^4.1.3",
    "custom-electron-titlebar": "^3.1.0",
    "dotenv": "^6.1.0",
    "electron-titlebar-windows": "^3.0.0",
    "font-awesome": "^4.7.0",
    "frameless-titlebar": "^1.0.8",
    "jquery": "^3.3.1",
    "office-ui-fabric-react": "6.49.3",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-excel-renderer": "^1.0.0",
    "react-file-viewer": "^1.1.0",
    "react-modal": "^3.10.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-tabs": "^3.0.0",
    "reactstrap": "^6.5.0",
    "typeface-ibm-plex-sans": "0.0.61",
    "xlsx": "0.13.4"
  }
}
